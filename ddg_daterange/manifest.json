{
    "manifest_version": 2,
    "name": "DuckDuckGo Date Range",
    "version": "1.0",
    
    "description": "Adds a daterange selector '&df=y' (past year) to DuckDuckGo search requests",

    "icons": {
        "48": "icons/icon48.png",
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_icon": "icon.png"
    }

}
